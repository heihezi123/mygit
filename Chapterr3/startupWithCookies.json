    "description": "这是一个会返回COOK信息的GET请求",
    "request": {
      "uri": "/getCookies",
      "method": "get"
    },
    "response": {
      "cookies": {
        "login": "true"
      },
      "text": "12345678910"
    }
  },
  {
    "description":"这是一个带cookies信息的get请求",
    "request": {
      "uri": "/get/with/cookies",
